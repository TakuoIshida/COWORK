commands:
  upgrade-aws-cli:
    command: |
      pip install awscli --upgrade --user
  set-instance-credit-unlimited:
    command: |
      aws ec2 modify-instance-credit-specification --region ap-northeast-1 --instance-credit-specification '[{"InstanceId": "i-0029191ef5c6feba0","CpuCredits": "unlimited"}]'
  